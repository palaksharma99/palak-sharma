#include <stdio.h>
int main()
{
int a,b;
printf("Enter two numbers");
scanf("%d%d",&a,&b);
printf("sum of a and b is: %d\n",a+b);
printf("subtract of a and b is: %d\n",a-b);
printf("Multiplication of a and b is: %d\n",a*b);
printf("Divide of a and b is: %d\n",a/b);
printf("modulusofaandbis:%d",a%b);
return0; }
