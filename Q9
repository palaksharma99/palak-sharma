#include <stdio.h>
int main()
{
int var1, var2, temp;
printf("Enter two integrs");
scanf("%d%d",&var1,&var2);
printf("Before Swapping First variable = %d\n Second variable = %d\n", var1, var2);
temp=var1;
var1 = var2;
var2=temp;
printf("After Swapping First variable = %d\n Second variable = %d\n", var1, var2);
return0;
}
