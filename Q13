#include <stdio.h>
intmain()
{
int week;
printf("Enter week number(1-7): ");
scanf("%d",&week);
switch(week)
{
case1:
printf("Monday");
break;
case2:
printf("Tuesday");
break;
case3:
printf("Wednesday");
break;
case4:
printf("Thursday");
break;
case5:
printf("Friday");
break;
case6:
printf("Saturday");
break;
case7:
printf("Sunday");
break;
default;
printf("Invalid input! Please enter week number between 1-7.");
}
return0;
}
